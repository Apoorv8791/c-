#include <iostream>
using namespace std;


int main()
{

int arr[]={5,55,748,100,2,54,1000};
int max=arr[0];
int n=sizeof(arr)/sizeof(arr[0]);
for(int i=0;i<n;i++)
{
    for(int j=j+1;j<n;j++)
    {
        if(arr[j]>max)
        {
            max=arr[j];
        }
    }
}
cout<<max;
    return 0;
}
