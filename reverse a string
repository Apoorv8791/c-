void revsere(string s)
{
    std::vector<string>vec ;
    string str;
    
    for(int i=0;i<s.size();i++)
    {
        if(s[i]==' ')
        {
            vec.push_back(str);
            str= " ";
        }
        else
        str=str+s[i];
    }
    vec.push_back(str);
    cout<<endl;
    for(int i=vec.size()-1;i>=0;i--)
    {
        cout<<vec[i]<<" ";
    }
}
